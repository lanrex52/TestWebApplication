                        <li class="sub"><i class="fa fa-spinner">&nbsp;&nbsp;&nbsp;</i>Subscription</li>
                    </a>
                    <a href="orders.html">
                        <li class="order"><i class="fa fa-shopping-basket">&nbsp;&nbsp;&nbsp;</i>Orders</li>
                    </a>
                    <a href="costCharge.html">
                        <li class="cost"><i class="fa fa-shopping-basket">&nbsp;&nbsp;&nbsp;</i>Customer Cost Charge</li>
                    </a>*@
                    <a href="index.html">
                        <li class="logout"><i class="fa fa-power-off">&nbsp;&nbsp;&nbsp;</i>Logout</li>
                    </a>
                </ul>


                
            </div>
        </div>
        <!-- End of NavigationBar -->
        <!-- AppContents -->
        <div class="appContents">
            <!-- ContentTopNav -->
            <div class="topNav">
                <div class="grid">
                    <div class="navCards toggle text-center">
                        <span class="fa fa-bars text-center">&nbsp;&nbsp;</span>
                    </div>
                    <script type="text/javascript">
                        $(document).ready(function () {
                            $('.toggle').click(function () {
                                $('.toggle').toggleClass('show')
                                $('.appNav').toggleClass('show')
                            })
                        })
                    </script>
                    <div class="navCards">
                        <h5>Dashboard</h5>
                    </div>

                    <div class="navCards">
                        <div class="circle">
                            <a ><i class="fa fa-search"></i></a>
                        </div>
                    </div>

                    <div class="navCards">
                        <div class="circle">
                            <a><i class="fa fa-envelope"></i><span class="badge badge-pill badge-danger">2</span></a>
                        </div>
                    </div>

                    <div class="navCards">
                        <div class="circle">
                            <a><i class="fa fa-bell"></i><span class="badge badge-pill badge-primary">2</span></a>
                        </div>
                    </div>

                    <div class="navCards">
                        <div class="circles">
                            <div class="circle">
                                <img src="~/Content/assets/ademola.png" alt="Ademola Akinbade" />
                            </div>
                            <a>Ademola Akinbade</a>
                        </div>
                    </div>

                </div>

            </div>
