<body>
    <!-- MainContainer -->
    <div class="app">
        <!-- NavigationBAr -->
        <div class="appNav">
            <div class="controls">
                <h6>Lotus Beta Analytics &nbsp;&nbsp; <span class="fa fa-times text-center toggle"></span></h6>
            </div>
            <div class="navLinks">
                <ul>
                    <a href="~/AdminUser/Index">
                        <li class="dashboard"><i class="fa fa-home">&nbsp;&nbsp;&nbsp;</i>Dashboard</li>
                    </a>
                    <a href="~/Customer/Index">
                        <li class="new"><i class="fa fa-user-plus">&nbsp;&nbsp;&nbsp;</i>Add End Customer</li>
                    </a>
                    <a href="~/Customer/CustomerManagement">
                        <li class="customer"><i class="fa fa-user-plus">&nbsp;&nbsp;&nbsp;</i>End Customer Management</li>
                    </a>
